import java.util.List;
import java.util.LinkedList;

public class LinkedListT {
 
	public static void main(String[] args) {
		List<String> list = new LinkedList<>();
		  list.add("Toy");
		  list.add("bix");
		  list.add("robo");
		 
		  for(int i =0; i <list.size(); i ++)
			 System.out.println(list.get(i));
		  System.out.println();
		  
		  list.remove(0);
		  
		  for(int i = 0 ; i < list.size(); i++)
			  System.out.println(list.get(i));
		  System.out.println();
			 
		 }



}
