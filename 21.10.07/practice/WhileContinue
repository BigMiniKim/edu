		int num = 0;
		int count = 0;
		
		while ((num++) < 100) {
			
			if (((num % 5) != 0) || ((num % 7) != 0))
				continue; // 나머지가 0이 아니면 다시 while로 cont, 5의 배수일때 7의 나머지 확인
			
			count++; //5 나머지 0 확인, 7나머지 0 확인, continue 다음. 35일때
			System.out.println(num);
		}
		
		System.out.println("count: " + count);
	}
