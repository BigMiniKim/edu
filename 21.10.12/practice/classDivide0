
public class DivideVoid {

	public static void main(String[] args) {
		divide(4, 2);
		divide(7, 3);
		divide(9, 0);
		divide(9, 3);
	}

	public static void divide(double num1, double num2) {
		if (num2 == 0) {
			System.out.println("0으로 나눌 수 없습니다.");
		} else {

			System.out.println("나눗셈 결과: " + (num1 / num2));
		}
	}
}
